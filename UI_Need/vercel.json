{
  "builds": [
    {
      "src": "Senti/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",  // Use camelCase for consistency
        "runtime": "python3.12.6"
      }
    }
  ],
  "cmd": "pip install -r requirements.txt && python manage.py collectstatic --noinput",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Senti/wsgi.py"
    }
  ]
}
